# DigitalBoda Rider App Environment Configuration Template
# Copy this file to .env and fill in your actual values

# API Configuration
API_BASE_URL=http://localhost:8000/api
# For production: https://your-api-domain.com/api

# Firebase Configuration (will be used if not using firebase_options.dart)
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_APP_ID=your-firebase-app-id
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
FIREBASE_STORAGE_BUCKET=your-storage-bucket

# Google Services Configuration
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# App Configuration
APP_NAME=DigitalBoda Rider
APP_VERSION=1.0.0
ENVIRONMENT=development

# Push Notifications
FCM_VAPID_KEY=your-fcm-vapid-key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_MODE=true

# Third-party Services
SENTRY_DSN=your-sentry-dsn-for-crash-reporting
MIXPANEL_TOKEN=your-mixpanel-token-for-analytics

# Development Settings
ENABLE_HTTP_LOGGING=true
API_TIMEOUT_SECONDS=30